<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="final.camp">
  <insert id="writeEvent" parameterType="greencamp.event.model.EventDTO">
  insert into tb_evnt values(seq_evnt_no.nextval,#{ctgry},#{sj},#{cn},#{pc},#{id},1)
  </insert>
  
  <select id="getListEventBsnmSQL" resultType="greencamp.event.model.EventDTO">
  select * from tb_evnt where id=#{id}
  
  </select>
  </mapper>